{
  "name": "lowban",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "babel-watch --exclude db.json app.js",
    "build": "webpack",
    "watch": "webpack --watch",
    "test": "jest --silent",
    "coverage": "jest --collect-coverage"
  },
  "babel": {
    "sourceMaps": "inline",
    "inputSourceMap": true,
    "retainLines": true,
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "chrome": "77",
            "node": "current"
          }
        }
      ],
      "@babel/preset-react",
      "jest"
    ]
  },
  "jest": {
    "transformIgnorePatterns": [
      "/node_modules/",
      "/lib/"
    ]
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "commonjs": true,
      "es6": true,
      "node": true,
      "jest": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
    },
    "parser": "babel-eslint",
    "parserOptions": {
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": 2018
    },
    "plugins": [
      "react",
      "react-hooks",
      "redux-saga"
    ],
    "rules": {
      "indent": [
        "error",
        2
      ],
      "linebreak-style": [
        "error",
        "unix"
      ],
      "quotes": [
        "error",
        "double"
      ],
      "semi": [
        "error",
        "never"
      ],
      "no-unused-vars": [
        "error",
        {
          "argsIgnorePattern": "^_"
        }
      ],
      "react-hooks/rules-of-hooks": "error",
      "react-hooks/exhaustive-deps": "warn"
    }
  },
  "author": "Cefn Hoile",
  "license": "GPL",
  "dependencies": {
    "@material-ui/core": "^4.5.1",
    "@material-ui/icons": "^4.5.1",
    "auto-bind": "^2.1.0",
    "bootstrap": "^4.3.1",
    "ejs": "^2.6.2",
    "express": "^4.17.1",
    "express-graphql": "^0.8.0",
    "express-promise-router": "^3.0.3",
    "fix-time": "^2.1.0",
    "graphql": "^14.4.1",
    "graphql-tools": "^4.0.5",
    "isomorphic-unfetch": "^3.0.0",
    "iter-tools": "^6.2.6",
    "json-schema-merge-allof": "^0.6.0",
    "json-schema-ref-parser": "^7.1.0",
    "json-stable-stringify": "^1.0.1",
    "lodash": "^4.17.15",
    "lodash-id": "^0.14.0",
    "lowdb": "^1.0.0",
    "react": "^16.9.0",
    "react-bootstrap": "^1.0.0-beta.12",
    "react-dom": "^16.9.0",
    "react-jsonschema-form": "^1.7.0",
    "react-redux": "^7.1.1",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.4",
    "redux-form": "^8.2.6",
    "redux-saga": "^1.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.6.0",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.6.0",
    "@types/jest": "^24.0.18",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-watch": "^7.0.0",
    "cross-env": "^5.2.0",
    "css-loader": "^3.2.0",
    "eslint": "^6.3.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jest": "^22.17.0",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^2.0.1",
    "eslint-plugin-redux-saga": "^1.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "jest": "^24.8.0",
    "jquery": "^3.4.1",
    "popper.js": "^1.15.0",
    "prop-types": "^15.7.2",
    "redux-saga-test-plan": "^4.0.0-rc.3",
    "style-loader": "^1.0.0",
    "webpack": "^4.39.2",
    "webpack-cli": "^3.3.7"
  }
}
